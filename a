display(){
console.log(this.body.speed);
if(this.body.speed < 2.5){
super.display();
}
else{
    World.remove(world, this.body);
    push();
    this.Visibility=this.Visibility-5;
    tint(255, this.Visibility);
    image(this.image, this.body.position.x, this.body.position.y, 50, 50);
pop();
  }
 }